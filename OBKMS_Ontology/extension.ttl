
@prefix trt: <http://plazi.org/treatment#> .
@prefix po: <http://www.essepuntato.it/2008/12/pattern#> .


trt:ScientificName rdf:type owl:Class ;
                   rdfs:label "Scientific Name"@en ;
                   rdfs:comment "A class for biological names"@en .

trt:relatedName rdf:type owl:ObjectProperty ,
						owl:TransitiveProperty ,
						owl:ReflexiveProperty ;
                  rdfs:domain trt:ScientificName ;
                  rdfs:range trt:ScientificName ;
                  rdfs:comment "Transitive property for names." .


po:contains rdf:type owl:ObjectProperty ,
			owl:TransitiveProperty .
