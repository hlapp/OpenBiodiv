      <tax:nomenclature obkms_id="9ed5a86b-0328-4c74-84a0-675a5bd79515" obkms_process="TRUE"><tax:name obkms_id="f280f209-5b68-4d6c-9f5d-aa7edb809e16" obkms_process="TRUE"><tax:xmldata><dwc:Kingdom>Animalia</dwc:Kingdom><dwc:Genus>Eisenia</dwc:Genus><dwc:Species>fetida</dwc:Species><dwc:taxonRank>species</dwc:taxonRank><dwc:scientificNameAuthorship>Savigny, 1826</dwc:scientificNameAuthorship></tax:xmldata>Eisenia fetida (Savigny,1826)</tax:name>Enterion fetidumSavigny,1826:182.</tax:nomenclature>
      <tax:ref_group>
        <tax:p><tax:name obkms_id="fe17f402-0fa0-495d-ae22-5878c08fb06f" obkms_process="TRUE"><tax:xmldata><dwc:Kingdom>Animalia</dwc:Kingdom><dwc:Genus>Lumbricus</dwc:Genus><dwc:Species>semifasciatus</dwc:Species><dwc:taxonRank>species</dwc:taxonRank><dwc:scientificNameAuthorship>Burmeister, 1835: 3</dwc:scientificNameAuthorship></tax:xmldata>Lumbricus semifasciatusBurmeister,1835:3</tax:name>.</tax:p>
