          <tp:taxon-name obkms_id="37de87e3-0417-400a-8ef4-8e74aa10e96f" obkms_process="TRUE">
            <tp:taxon-name-part taxon-name-part-type="genus">Melicope</tp:taxon-name-part>
            <tp:taxon-name-part taxon-name-part-type="species">frutescens</tp:taxon-name-part>
            <object-id content-type="ipni" xlink:type="simple">urn:lsid:ipni.org:names:77151881-1</object-id>
          </tp:taxon-name>
