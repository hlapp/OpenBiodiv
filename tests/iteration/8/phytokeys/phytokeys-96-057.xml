            </caption>
            <graphic xlink:href="phytokeys-96-057-g001.jpg" position="float" orientation="portrait" xlink:type="simple" id="oo_193638.jpg"/>
          </fig>
          <fig id="F2" position="float" orientation="portrait" obkms_id="56b78ed2-c151-4b7f-b0a4-55447aea5a08" obkms_process="TRUE">
