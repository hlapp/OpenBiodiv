                                    <th rowspan="1" colspan="1">Tarsus</th>
                                    <th rowspan="1" colspan="1">Total</th>
                                </tr>
                                <tr>
                                    <td rowspan="1" colspan="1">I</td>
                                    <td rowspan="1" colspan="1">2.03</td>
                                    <td rowspan="1" colspan="1">0.75</td>
                                    <td rowspan="1" colspan="1">1.83</td>
                                    <td rowspan="1" colspan="1">2.03</td>
