        </tp:treatment-meta>
        <tp:nomenclature obkms_id="1346a323-87ca-4f5a-bbf6-21d76ae3bddc" obkms_process="TRUE">
          <tp:taxon-name obkms_id="979dc6b1-fa9c-4c64-9f8a-1c3cec609c3e" obkms_process="TRUE">
            <tp:taxon-name-part taxon-name-part-type="genus">Callicarpa</tp:taxon-name-part>
            <tp:taxon-name-part taxon-name-part-type="species">bachmaensis</tp:taxon-name-part>
