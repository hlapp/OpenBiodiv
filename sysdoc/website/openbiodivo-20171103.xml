    <!-- http://purl.org/dc/elements/1.1/modified -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/modified"/>
    


    <!-- http://purl.org/dc/elements/1.1/replaces -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/replaces"/>
    


    <!-- http://purl.org/spar/fabio/hasDiscipline -->

    <owl:AnnotationProperty rdf:about="http://purl.org/spar/fabio/hasDiscipline"/>
    


    <!-- http://rs.tdwg.org/dwc/terms/attributes/abcdEquivalence -->

    <owl:AnnotationProperty rdf:about="http://rs.tdwg.org/dwc/terms/attributes/abcdEquivalence"/>
    


    <!-- http://rs.tdwg.org/dwc/terms/attributes/decision -->

    <owl:AnnotationProperty rdf:about="http://rs.tdwg.org/dwc/terms/attributes/decision"/>
    


    <!-- http://rs.tdwg.org/dwc/terms/attributes/status -->

    <owl:AnnotationProperty rdf:about="http://rs.tdwg.org/dwc/terms/attributes/status"/>
    


    <!-- http://www.w3.org/2003/06/sw-vocab-status/ns#term_status -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2003/06/sw-vocab-status/ns#term_status"/>
    


