    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#altLabel"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#prefLabel -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#prefLabel"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://openbiodiv.net/contains -->

    <owl:ObjectProperty rdf:about="http://openbiodiv.net/contains">
        <rdfs:subPropertyOf rdf:resource="http://www.essepuntato.it/2008/12/pattern#contains"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:label>OpenBiodiv contains</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://openbiodiv.net/has_OTU_Id -->

    <owl:ObjectProperty rdf:about="http://openbiodiv.net/has_OTU_Id">
        <rdfs:subPropertyOf rdf:resource="http://openbiodiv.net/scientificName"/>
        <rdfs:domain rdf:resource="http://openbiodiv.net/TaxonomicConcept"/>
        <rdfs:range rdf:resource="http://openbiodiv.net/OTU_Id"/>
        <rdfs:label xml:lang="en">has OTU Id</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://openbiodiv.net/isContainedBy -->

    <owl:ObjectProperty rdf:about="http://openbiodiv.net/isContainedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.essepuntato.it/2008/12/pattern#isContainedBy"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:label>OpenBiodiv is contained by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://openbiodiv.net/mentions -->

    <owl:ObjectProperty rdf:about="http://openbiodiv.net/mentions">
        <rdfs:domain rdf:resource="http://openbiodiv.net/Mention"/>
        <rdfs:comment>A direct link between an information resource, like a document or a section and an entity.</rdfs:comment>
        <rdfs:label>mentions</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://openbiodiv.net/nameAccordingTo -->

    <owl:ObjectProperty rdf:about="http://openbiodiv.net/nameAccordingTo">
        <rdfs:subPropertyOf rdf:resource="http://rs.tdwg.org/dwc/iri/nameAccordingTo"/>
        <rdfs:domain rdf:resource="http://openbiodiv.net/TaxonomicConceptLabel"/>
        <rdfs:range rdf:resource="http://purl.org/vocab/frbr/core#Expression"/>
        <rdfs:comment xml:lang="en">The reference to the source in which the specific taxonomic 
    concept circumscription is defined or implied - traditionally signified by
    the Latin &apos;sensu&apos; or &apos;sec.&apos;&apos; (from secundum, meaning &apos;according to&apos;).
    For taxa that are relevantly circumscribed by identifications, a reference
    to the keys, monographs, experts and other sources should be given. Should
    only be used with IRI&apos;s</rdfs:comment>
        <skos:altLabel xml:lang="en">sensu</skos:altLabel>
        <skos:prefLabel xml:lang="en">OpenBiodiv name according to</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://openbiodiv.net/rcc5AccrodingTo -->

    <owl:ObjectProperty rdf:about="http://openbiodiv.net/rcc5AccrodingTo">
        <rdfs:subPropertyOf rdf:resource="http://openbiodiv.net/rcc5Property"/>
        <rdfs:range rdf:resource="http://purl.org/vocab/frbr/core#Expression"/>
        <rdfs:comment xml:lang="en">In which publication was the statement made</rdfs:comment>
        <rdfs:label>RCC5 statement according to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://openbiodiv.net/rcc5Property -->

    <owl:ObjectProperty rdf:about="http://openbiodiv.net/rcc5Property">
        <rdfs:domain rdf:resource="http://openbiodiv.net/RCC5Statement"/>
    </owl:ObjectProperty>
    


    <!-- http://openbiodiv.net/rcc5RelationType -->

    <owl:ObjectProperty rdf:about="http://openbiodiv.net/rcc5RelationType">
        <rdfs:subPropertyOf rdf:resource="http://openbiodiv.net/rcc5Property"/>
        <rdfs:range rdf:resource="http://openbiodiv.net/RCC5Relation"/>
        <rdfs:comment xml:lang="en">Connects an RCC-5 statement to the type of RCC-5 relation between the regions.</rdfs:comment>
        <rdfs:label>relation type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://openbiodiv.net/rcc5fromRegion -->

    <owl:ObjectProperty rdf:about="http://openbiodiv.net/rcc5fromRegion">
        <rdfs:subPropertyOf rdf:resource="http://openbiodiv.net/rcc5Property"/>
        <rdfs:comment xml:lang="en">Connects to the RCC5 statement to the originating region</rdfs:comment>
        <rdfs:label>from region</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://openbiodiv.net/rcc5toRegion -->

    <owl:ObjectProperty rdf:about="http://openbiodiv.net/rcc5toRegion">
        <rdfs:subPropertyOf rdf:resource="http://openbiodiv.net/rcc5Property"/>
        <rdfs:comment xml:lang="en">Connects to the RCC5 statement to the target region</rdfs:comment>
        <rdfs:label>to region</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://openbiodiv.net/relatedName -->

    <owl:ObjectProperty rdf:about="http://openbiodiv.net/relatedName">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ReflexiveProperty"/>
        <rdfs:domain rdf:resource="http://openbiodiv.net/TaxonomicName"/>
        <rdfs:range rdf:resource="http://openbiodiv.net/TaxonomicName"/>
        <rdfs:comment xml:lang="en">&apos;has related name&apos; is an object property that we
    use in order to indicate that two taxonomic names are related somehow. This
    relationship is purposely vague as to encompass all situations where two
    taxonomic names co-occur in a text. It is transitive and reflexive.</rdfs:comment>
        <rdfs:label xml:lang="en">has related name</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://openbiodiv.net/replacementName -->

    <owl:ObjectProperty rdf:about="http://openbiodiv.net/replacementName">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://openbiodiv.net/LatinName"/>
        <rdfs:range rdf:resource="http://openbiodiv.net/LatinName"/>
        <rdfs:comment xml:lang="en">This is a uni-directional property. Its meaning
    is that one Linnaean name links to a different Linnaean name via the
    usage of this property, then the object name is more accurate and should be
    preferred given the information that system currently holds. This property is only
    defined for Linnaean names.</rdfs:comment>
        <rdfs:label xml:lang="en">has replacement name</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://openbiodiv.net/scientificName -->

    <owl:ObjectProperty rdf:about="http://openbiodiv.net/scientificName">
        <rdfs:subPropertyOf rdf:resource="http://openbiodiv.net/taxonomicName"/>
        <rdfs:subPropertyOf rdf:resource="http://rs.tdwg.org/dwc/iri/scientificName"/>
        <rdfs:domain rdf:resource="http://openbiodiv.net/TaxonomicConcept"/>
        <rdfs:range rdf:resource="http://openbiodiv.net/ScientificName"/>
        <rdfs:label xml:lang="en">has OpeonBiodiv scientific name</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://openbiodiv.net/taxonomicConceptLabel -->

    <owl:ObjectProperty rdf:about="http://openbiodiv.net/taxonomicConceptLabel">
        <rdfs:subPropertyOf rdf:resource="http://openbiodiv.net/scientificName"/>
        <rdfs:domain rdf:resource="http://openbiodiv.net/TaxonomicConcept"/>
        <rdfs:range rdf:resource="http://openbiodiv.net/TaxonomicConceptLabel"/>
        <rdfs:label xml:lang="en">has taxon concept label</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://openbiodiv.net/taxonomicName -->

    <owl:ObjectProperty rdf:about="http://openbiodiv.net/taxonomicName">
        <rdfs:domain rdf:resource="http://openbiodiv.net/TaxonomicConcept"/>
        <rdfs:range rdf:resource="http://openbiodiv.net/TaxonomicName"/>
        <rdfs:label xml:lang="en">has OpenBiodiv taxonomic name</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/spar/fabio/isSchemeOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/spar/fabio/isSchemeOf"/>
    


    <!-- http://rs.tdwg.org/dwc/iri/nameAccordingTo -->

    <owl:ObjectProperty rdf:about="http://rs.tdwg.org/dwc/iri/nameAccordingTo">
        <rdfs:comment xml:lang="en">the IRI version of dwc:nameAccordingTo</rdfs:comment>
        <rdfs:label xml:lang="en">name according to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://rs.tdwg.org/dwc/iri/scientificName -->

    <owl:ObjectProperty rdf:about="http://rs.tdwg.org/dwc/iri/scientificName">
        <rdfs:comment xml:lang="en">the IRI version of dwc:scientificName</rdfs:comment>
        <rdfs:label xml:lang="en">has scientific name</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://rs.tdwg.org/dwc/iri/taxonomicStatus -->

    <owl:ObjectProperty rdf:about="http://rs.tdwg.org/dwc/iri/taxonomicStatus">
        <rdfs:comment xml:lang="en">the IRI version of the DwC term taxonomic status</rdfs:comment>
        <rdfs:label xml:lang="en">taxonomic status</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.essepuntato.it/2008/12/pattern#contains -->

    <owl:ObjectProperty rdf:about="http://www.essepuntato.it/2008/12/pattern#contains">
        <owl:inverseOf rdf:resource="http://www.essepuntato.it/2008/12/pattern#isContainedBy"/>
        <rdfs:domain rdf:resource="http://www.essepuntato.it/2008/12/pattern#Structured"/>
        <rdfs:comment xml:lang="en">A structured element contains another generic element.</rdfs:comment>
        <rdfs:label xml:lang="en">contains</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.essepuntato.it/2008/12/pattern#isContainedBy -->

    <owl:ObjectProperty rdf:about="http://www.essepuntato.it/2008/12/pattern#isContainedBy">
        <rdfs:comment xml:lang="en">An element that is contained by another structured element.</rdfs:comment>
        <rdfs:label xml:lang="en">is contained by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/2004/02/skos/core#inScheme -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#inScheme"/>
    


    <!-- 
