    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://openbiodiv.net/BiologySection -->

    <owl:Class rdf:about="http://openbiodiv.net/BiologySection">
        <rdfs:subClassOf rdf:resource="http://purl.org/spar/deo/DiscourseElement"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://openbiodiv.net/isContainedBy"/>
                <owl:someValuesFrom rdf:resource="http://openbiodiv.net/Treatment"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Biology Section</rdfs:label>
    </owl:Class>
    


    <!-- http://openbiodiv.net/DescriptionSection -->

    <owl:Class rdf:about="http://openbiodiv.net/DescriptionSection">
        <rdfs:subClassOf rdf:resource="http://purl.org/spar/deo/DiscourseElement"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://openbiodiv.net/isContainedBy"/>
                <owl:someValuesFrom rdf:resource="http://openbiodiv.net/Treatment"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Description Section</rdfs:label>
    </owl:Class>
    


    <!-- http://openbiodiv.net/DiagnosisSection -->

    <owl:Class rdf:about="http://openbiodiv.net/DiagnosisSection">
        <rdfs:subClassOf rdf:resource="http://purl.org/spar/deo/DiscourseElement"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://openbiodiv.net/isContainedBy"/>
                <owl:someValuesFrom rdf:resource="http://openbiodiv.net/Treatment"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Diagnosis Section</rdfs:label>
    </owl:Class>
    


    <!-- http://openbiodiv.net/DistributionSection -->

    <owl:Class rdf:about="http://openbiodiv.net/DistributionSection">
        <rdfs:subClassOf rdf:resource="http://purl.org/spar/deo/DiscourseElement"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://openbiodiv.net/isContainedBy"/>
                <owl:someValuesFrom rdf:resource="http://openbiodiv.net/Treatment"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Distribution Section</rdfs:label>
    </owl:Class>
    


    <!-- http://openbiodiv.net/EtymologySection -->

    <owl:Class rdf:about="http://openbiodiv.net/EtymologySection">
        <rdfs:subClassOf rdf:resource="http://purl.org/spar/deo/DiscourseElement"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://openbiodiv.net/isContainedBy"/>
                <owl:someValuesFrom rdf:resource="http://openbiodiv.net/Treatment"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Etymology Section</rdfs:label>
    </owl:Class>
    


    <!-- http://openbiodiv.net/LatinName -->

    <owl:Class rdf:about="http://openbiodiv.net/LatinName">
        <rdfs:subClassOf rdf:resource="http://openbiodiv.net/ScientificName"/>
        <skos:altLabel xml:lang="en">Linnaean Name</skos:altLabel>
        <skos:prefLabel xml:lang="en">Latinized Scientific Name</skos:prefLabel>
    </owl:Class>
    


    <!-- http://openbiodiv.net/MaterialsExamined -->

    <owl:Class rdf:about="http://openbiodiv.net/MaterialsExamined">
        <rdfs:subClassOf rdf:resource="http://purl.org/spar/deo/DiscourseElement"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://openbiodiv.net/isContainedBy"/>
                <owl:someValuesFrom rdf:resource="http://openbiodiv.net/Treatment"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Materials Examined</rdfs:label>
    </owl:Class>
    


    <!-- http://openbiodiv.net/Mention -->

    <owl:Class rdf:about="http://openbiodiv.net/Mention">
        <rdfs:subClassOf rdf:resource="http://purl.org/spar/deo/DiscourseElement"/>
        <rdfs:comment xml:lang="en">An area of a document that can be considered a mention of something.</rdfs:comment>
        <rdfs:label>Mention</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://proton.semanticweb.org/protonue#Mention"/>
    </owl:Class>
    


    <!-- http://openbiodiv.net/NomenclatureCitationsList -->

    <owl:Class rdf:about="http://openbiodiv.net/NomenclatureCitationsList">
        <rdfs:subClassOf rdf:resource="http://purl.org/spar/deo/DiscourseElement"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://openbiodiv.net/isContainedBy"/>
                <owl:someValuesFrom rdf:resource="http://openbiodiv.net/NomenclatureSection"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A section in a treatment that includes the citation of one or
    several previous treatments of the taxon.</rdfs:comment>
        <rdfs:label xml:lang="en">Nomenclature Citations List</rdfs:label>
    </owl:Class>
    


    <!-- http://openbiodiv.net/NomenclatureHeading -->

    <owl:Class rdf:about="http://openbiodiv.net/NomenclatureHeading">
        <rdfs:subClassOf rdf:resource="http://purl.org/spar/deo/DiscourseElement"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://openbiodiv.net/isContainedBy"/>
                <owl:someValuesFrom rdf:resource="http://openbiodiv.net/NomenclatureSection"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Inside the taxonomic nomenclature section, we have the treatment
    title (name of the taxon).</rdfs:comment>
        <rdfs:label xml:lang="en">Nomenclature Heading</rdfs:label>
    </owl:Class>
    


    <!-- http://openbiodiv.net/NomenclatureSection -->

    <owl:Class rdf:about="http://openbiodiv.net/NomenclatureSection">
        <rdfs:subClassOf rdf:resource="http://purl.org/spar/deo/DiscourseElement"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://openbiodiv.net/isContainedBy"/>
                <owl:someValuesFrom rdf:resource="http://openbiodiv.net/Treatment"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A section of a taxonomic treatment, containing the scientific
    name of the taxon described by the treatment, and citations to previous
    descriptions, designations of type-genus, and type-species,
    and other information.</rdfs:comment>
        <rdfs:label xml:lang="en">Nomenclature Section</rdfs:label>
    </owl:Class>
    


    <!-- http://openbiodiv.net/OTU_Id -->

    <owl:Class rdf:about="http://openbiodiv.net/OTU_Id">
        <rdfs:subClassOf rdf:resource="http://openbiodiv.net/ScientificName"/>
        <rdfs:comment xml:lang="en">The identifier of an Operational Taxonomic Unit, such as
    BOLD BIN or a Unite SH ID</rdfs:comment>
        <rdfs:label xml:lang="en">Operational Taxonomic Unit Identifier</rdfs:label>
    </owl:Class>
    


    <!-- http://openbiodiv.net/OperationalTaxonomicUnit -->

    <owl:Class rdf:about="http://openbiodiv.net/OperationalTaxonomicUnit">
        <rdfs:subClassOf rdf:resource="http://purl.org/spar/fabio/Work"/>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <rdfs:comment xml:lang="en">A superclass for all kinds of taxonomic hypothesis</rdfs:comment>
        <rdfs:label xml:lang="en">Operational Taxonomic Unit</rdfs:label>
    </owl:Class>
    


    <!-- http://openbiodiv.net/Publisher -->

    <owl:Class rdf:about="http://openbiodiv.net/Publisher">
        <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
        <rdfs:comment xml:lang="en">The publisher of a journal, a type of `foaf:Agent`.</rdfs:comment>
        <rdfs:label xml:lang="en">Publisher</rdfs:label>
    </owl:Class>
    


