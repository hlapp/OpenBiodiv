     xmlns:inst="http://www.ontotext.com/connectors/lucene/instance#"
     xmlns:trt="http://plazi.org/treatment#"
     xmlns:nomen="http://www.semanticweb.org/dmitriev/ontologies/2013/8/untitled-ontology-6#"
     xmlns:vs="http://www.w3.org/2003/06/sw-vocab-status/ns#"
     xmlns:po="http://www.essepuntato.it/2008/12/pattern#">
    <owl:Ontology rdf:about="http://openbiodiv.net/openbiodiv-ontology">
        <owl:imports rdf:resource="http://purl.org/spar/fabio/"/>
        <dcelements:creator>Viktor Senderov, Kiril Simov</dcelements:creator>
        <dcelements:title>The Open Biodiversity Knowledge Management System Ontology</dcelements:title>
        <dcelements:subject>Biodiversity Informatics</dcelements:subject>
        <rdfs:comment>You are free to create derivative work as long as you cite the original authors.</rdfs:comment>
        <owl:versionInfo>1.0</owl:versionInfo>
        <dcelements:license>CC-BY license version 4.0</dcelements:license>
        <rdfs:label>OpenBiodiv Ontology</rdfs:label>
    </owl:Ontology>
    


